<div>
 <!-- Navigation -->

  <div class="b-nav">
    <li><a class="b-link b-link--active" href="/">Home</a></li>
    <li><a class="b-link" href="#">About</a></li>
    <li><a class="b-link" href="/events">Events</a></li>
        <% if (currentUser) { %>
          <li><a class="b-link" href="/show"><%= currentUser.local.email %></a></li>
          <li><a class="b-link" href="/logout">Logout</a></li>
        <% } else { %>
